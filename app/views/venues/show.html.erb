<% @posts.each do |post| %>
<div class="col-sm-6">
  <div class="panel panel-primary">
    <div class="panel-heading">
      <h3 class="panel-title">TripTip</h3>
    </div>
    <div class="panel-body">
      <h5><p><%=post.text%></p>
      Give it about <%=post.time%> hours.</h5>
      by <%=post.user.name%>, <%=post.friendly_time%>
    </div>
  </div>
</div>
<% end %>  

<%= render "shared/details", url: "/itineraries/#{@itinerary.id}/venues" %>

